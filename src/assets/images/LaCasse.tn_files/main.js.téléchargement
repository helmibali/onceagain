const httpOPtions = {
    headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_0__["HttpHeaders"]({ 'Content-Type': 'application/json' })
};
class CommentService {
    constructor(authservice, http) {
        this.authservice = authservice;
        this.http = http;
    }
    addComment(formData) {
        return this.http.post('api/comment1', formData);
    }
    getComments() {
        return this.http.get('api/comments');
    }
    createComment(text, parentId, articleId) {
        return this.http.post('api/comment', {
            text: text,
            user: this.authservice.loggedUser,
            article: articleId,
            parentId,
            dateComment: new Date(),
        });
    }
    listeComments() {
        return this.http.get('api/comments');
    }
    deleteComment(id) {
        const url = `${'api/comment'}/${id}`;
        return this.http.delete(url, httpOPtions);
    }
    updateComment(id, text, parentId, articleId) {
        return this.http.put(`'api/comment'/${id}`, {
            id,
            text: text,
            user: this.authservice.loggedUser,
            article: articleId,
            parentId,
            dateComment: new Date(),
        });
    }
}
