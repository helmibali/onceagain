class LoginComponent {
    constructor(authService, router) {
        this.authService = authService;
        this.router = router;
        this.user = new src_app_model_user_model__WEBPACK_IMPORTED_MODULE_1__["User"]();
        this.err = 0;
        this.erreur = 0;
        this.isLoading = false;
        this.errTXT = '';
    }
    onLoggedin() {
        this.authService.getUserFromDB(this.user.username).subscribe(usr => {
            if (usr.password == this.user.password) {
                this.authService.signIn(usr);
                this.router.navigate(['/']);
            }
            else
                this.erreur = 1;
            this.errTXT = "login ou mot de passe invalid";
        }, (err) => {
            this.err = 1;
            console.log(err);
        });
    }
    ngOnInit() {
    }
}
