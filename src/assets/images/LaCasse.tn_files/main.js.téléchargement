    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵproperty"]("a", ctx_r0.a)("cmt", cmt_r1)("replies", ctx_r0.getReplies(cmt_r1.id))("activeComment", ctx_r0.activeComment);
