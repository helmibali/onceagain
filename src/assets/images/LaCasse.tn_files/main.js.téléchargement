class UpdateCategorieComponent {
    constructor(catService, activatedRoute, router) {
        this.catService = catService;
        this.activatedRoute = activatedRoute;
        this.router = router;
        this.currentCat = new src_app_model_categorie_model__WEBPACK_IMPORTED_MODULE_1__["Categorie"]();
    }
    ngOnInit() {
        this.catService.consulterCategorie(this.activatedRoute.snapshot.params.id)
            .subscribe(cat => {
            this.currentCat = cat;
        });
    }
    updateCategorie() {
        this.catService.updateCategorie(this.currentCat, this.currentCat.id).subscribe(cat => {
            this.router.navigate(['categories']);
        }, (error) => { alert('Probléme lors de la modification'); });
    }
}
