/***/ "ccyI":
/*!*********************************!*\
  !*** ./src/app/auth.service.ts ***!
  \*********************************/
/*! exports provided: AuthService */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "AuthService", function() { return AuthService; });
/* harmony import */ var _angular_common_http__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @angular/common/http */ "tk/3");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/router */ "tyNb");





const httpOPtions = {
    headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_0__["HttpHeaders"]({ 'Content-Type': 'application/json' })
};
class AuthService {
    constructor(router, http) {
        this.router = router;
        this.http = http;
        this.isloggedIn = false;
        this.apiURL = '/api/login';
        this.apiURLall = '/api/users/liste';
    }
    getUserFromDB(username) {
        const url = `${this.apiURL}/${username}`;
        return this.http.get(url);
    }
    getUserslist() {
        return this.http.get(this.apiURLall);
    }
    setLoggedUserFromLocalStorage(login) {
        this.loggedUser = login;
        this.isloggedIn = true;
        this.getUserRoles(login);
    }
    signIn(user) {
        this.loggedUser = user.username;
        this.isloggedIn = true;
        this.roles = user.roles;
        localStorage.setItem('loggedUser', this.loggedUser);
        localStorage.setItem('isloggedIn', String(this.isloggedIn));
    }
    isAdmin() {
        let admin = false;
        if (!this.roles) //this.roles== undefiened
            return false;
        this.roles.forEach((curRole) => {
            if (curRole.role == 'ADMIN') {
                admin = true;
            }
        });
        return admin;
    }
    getUserRoles(username) {
        this.getUserFromDB(username).subscribe((user) => {
            this.roles = user.roles;
        });
    }
    addUser(user) {
        return this.http.post('/api/user/add', user, httpOPtions);
    }
    logout() {
        this.isloggedIn = false;
        this.loggedUser = undefined;
        this.roles = undefined;
        localStorage.removeItem('loggedUser');
        localStorage.setItem('isloggedIn', String(this.isloggedIn));
        this.router.navigate(['/login']);
    }
}
AuthService.ɵfac = function AuthService_Factory(t) { return new (t || AuthService)(_angular_core__WEBPACK_IMPORTED_MODULE_1__["ɵɵinject"](_angular_router__WEBPACK_IMPORTED_MODULE_2__["Router"]), _angular_core__WEBPACK_IMPORTED_MODULE_1__["ɵɵinject"](_angular_common_http__WEBPACK_IMPORTED_MODULE_0__["HttpClient"])); };
AuthService.ɵprov = _angular_core__WEBPACK_IMPORTED_MODULE_1__["ɵɵdefineInjectable"]({ token: AuthService, factory: AuthService.ɵfac, providedIn: 'root' });
/*@__PURE__*/ (function () { _angular_core__WEBPACK_IMPORTED_MODULE_1__["ɵsetClassMetadata"](AuthService, [{
