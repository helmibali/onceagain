class ListeCategorieComponent {
    constructor(catService, authService, router) {
        this.catService = catService;
        this.authService = authService;
        this.router = router;
    }
    ngOnInit() {
        this.catService.listeCategories().subscribe(cats => {
            console.log(cats);
            this.categories = cats;
        });
    }
    supprimerCategorie(c) {
        let conf = confirm("Etes vous sur ?");
        if (conf)
            this.catService.supprimerCategorie(c.id).subscribe(() => {
                console.log("Catégorie supprimé");
            });
        this.router.navigate(['categories']).then(() => {
            window.location.reload();
        });
    }
}
