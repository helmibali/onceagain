class ListMarqueComponent {
    constructor(authService, marqueService, router, fb) {
        this.authService = authService;
        this.marqueService = marqueService;
        this.router = router;
        this.fb = fb;
        this.control = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]('');
    }
    ngOnInit() {
        this.marqueService.getAll().subscribe(mq => {
            console.log(mq);
            this.marques = mq;
        });
    }
    supprimerMarque(m) {
        let conf = confirm("Etes vous sur ?");
        if (conf)
            this.marqueService.supprimerProduit(m.id).subscribe(() => {
                console.log("marque supprimé");
            });
        this.router.navigate(['marques']).then(() => {
            window.location.reload();
        });
    }
}
