class UtilisateursComponent {
    constructor(api, router, userService) {
        this.api = api;
        this.router = router;
        this.userService = userService;
        this.users = [];
        this.isLoading = false;
        this.errTXT = '';
    }
    ngOnInit() {
        this.initData();
    }
    initData() {
        this.isLoading = true;
        this.api.getUserslist().toPromise().then((res) => {
            console.log(res);
            this.users = res;
        })
            .catch(() => this.errTXT = "msg d'erreur").finally(() => {
            this.isLoading = false;
        });
    }
    supprimerUtilisateur(u) {
        let conf = confirm("Etes vous sur ?");
        if (conf)
            this.userService.supprimerUtilisateur(u.user_id).subscribe(() => {
                console.log("produit supprimé");
            });
        this.router.navigate(['/utilisateurs']).then(() => {
            window.location.reload();
        });
    }
}
