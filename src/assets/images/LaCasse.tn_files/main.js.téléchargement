class CommentComponent {
    constructor(commentService, authService) {
        this.commentService = commentService;
        this.authService = authService;
        this.setActiveComment = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.deleteComment = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.addComment = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.updateComment = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.createdAt = '';
        this.canReply = false;
        this.canEdit = false;
        this.canDelete = false;
        this.activeCommentType = src_app_model_activeCommentTypeEnum__WEBPACK_IMPORTED_MODULE_1__["ActiveCommentTypeEnum"];
        this.replyId = null;
        this.currentUsername = this.authService.loggedUser;
    }
    ngOnInit() {
        const fiveMinutes = 300000;
        let date = new Date();
        let dateComment = new Date(this.cmt.dateComment);
        const timePassed = date.getTime() - dateComment.getTime() > fiveMinutes;
        this.canReply = Boolean(this.currentUsername);
        this.canEdit = this.currentUsername === this.cmt.user.username && !timePassed;
        this.canDelete = this.currentUsername === this.cmt.user.username &&
            this.replies.length === 0;
        this.replyId = this.parentId ? this.parentId : this.cmt.id;
    }
    isReplying() {
        if (!this.activeComment) {
            return false;
        }
        return (this.activeComment.id === this.cmt.id &&
            this.activeComment.type === this.activeCommentType.replying);
    }
}
