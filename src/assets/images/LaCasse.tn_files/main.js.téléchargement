class ArtComponent {
    constructor(articleService) {
        this.articleService = articleService;
    }
    ngOnInit() {
        this.articleService.listeArticles().subscribe(a => {
            console.log(a);
            this.articles = a;
        });
    }
    articlesBydate() {
        return this.articles
            .sort((a, b) => new Date(b.dateCreation).getTime() - new Date(a.dateCreation).getTime());
    }
}
