        return this.http.get('/api/categorie/liste');
    }
    consulterCategorie(id) {
        const url = `${this.apiURL}/${id}`;
        return this.http.get(url);
    }
    updateCategorie(cat, id) {
        const url = `${this.apiURLu}/${id}`;
        return this.http.put(url, cat, httpOPtions);
    }
    ajouterCategorie(cat) {
        return this.http.post('/api/categorie/add', cat, httpOPtions);
    }
    supprimerCategorie(id) {
        const url = `${this.apiURLd}/${id}`;
        return this.http.delete(url, httpOPtions);
    }
    listeCategorieByFamille(id) {
        const url = `${'/api/categoriesByFamille'}/${id}`;
        return this.http.get(url);
    }
