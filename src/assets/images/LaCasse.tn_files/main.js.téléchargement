};
class MarqueService {
    constructor(http) {
        this.http = http;
    }
    uploadFile(file) {
        const formdata = new FormData();
        formdata.append('file', file);
        const req = new _angular_common_http__WEBPACK_IMPORTED_MODULE_0__["HttpRequest"]('POST', '<Server URL of the file upload>', formdata, {
            reportProgress: true,
            responseType: 'text'
        });
        return this.http.request(req);
    }
    createData(formData) {
        return this.http.post("/api/addMarque", formData);
    }
    getData(id) {
        return this.http.get(`${'/api/marque'}/${id}`);
    }
    updateMarque(formData) {
        return this.http.put("/api/marque", formData);
    }
    deleteData(id) {
        return this.http.delete(`${this.baseUrl}/${id}`, { responseType: 'text' });
    }
    getAll() {
        return this.http.get('/api/marque/liste');
    }
    supprimerProduit(id) {
        const url = `${'/api/marque'}/${id}`;
        return this.http.delete(url, httpOPtions);
    }
    consulterMarque(id) {
        const url = `${'/api/marque'}/${id}`;
        return this.http.get(url);
    }
    getAllMarques() {
        return this.http.get('/api/marque/liste');
    }
}
