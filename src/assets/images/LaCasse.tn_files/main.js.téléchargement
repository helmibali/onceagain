        });
    }
    supprimerProduit(p) {
        let conf = confirm("Etes vous sur ?");
        if (conf)
            this.produitService.supprimerProduit(p.idProduit).subscribe(() => {
                console.log("produit supprimé");
            });
        this.router.navigate(['produits']).then(() => {
            window.location.reload();
        });
    }
}
