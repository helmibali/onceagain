class ModeleService {
    constructor(http) {
        this.http = http;
    }
    getAll() {
        return this.http.get('/api/modele/liste');
    }
    consulterModele(id) {
        const url = `${this.apiUrl}/${id}`;
        return this.http.get(url);
    }
    updateCategorie(m, id) {
        const url = `${this.apiUrl}/${id}`;
        return this.http.put(url, m, httpOPtions);
    }
    ajouterCategorie(formData) {
        return this.http.post('api/modele/add', formData);
    }
    supprimerCategorie(id) {
        const url = `${this.apiUrl}/${id}`;
        return this.http.delete(url, httpOPtions);
    }
    getAllModeles() {
        return this.http.get('/api/modele/liste');
    }
    getAllModelesByMarque_id(id) {
        return this.http.get(`${'/api/modeleByMarqueId'}/${id}`);
    }
}
